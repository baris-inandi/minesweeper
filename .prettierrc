{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 80,
	"semi": true,
	"quoteProps": "consistent",
	"bracketSameLine": true,
	"plugins": [
		"prettier-plugin-svelte"
	],
	"pluginSearchDirs": [
		"."
	],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}